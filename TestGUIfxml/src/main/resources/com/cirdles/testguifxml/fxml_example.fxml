<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane minHeight="450.0" minWidth="400.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cirdles.testguifxml.FXMLExampleController">
   <children>
      <GridPane alignment="center" hgap="10" minHeight="200.0" minWidth="100.0" prefHeight="450.0" prefWidth="350.0" vgap="10" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
         <columnConstraints>
            <ColumnConstraints percentWidth="50.0" />
            <ColumnConstraints minWidth="10.0" percentWidth="25.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints percentHeight="50.0" />
            <RowConstraints />
            <RowConstraints />
         </rowConstraints>
         <children>
            <Button fx:id="showBtn" mnemonicParsing="false" onAction="#showPokemon" prefHeight="34.0" prefWidth="83.0" text="Show" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
            <TextField fx:id="nameField" promptText="Type Pokemon" GridPane.rowIndex="1" />
            <Label alignment="CENTER" prefHeight="47.0" prefWidth="301.0" text="Type The Name Of Your Pokemon" GridPane.columnSpan="2" GridPane.halignment="CENTER" />
            <ImageView fx:id="pokemon" fitHeight="127.0" fitWidth="157.0" pickOnBounds="true" preserveRatio="true" GridPane.halignment="CENTER" GridPane.rowIndex="3" GridPane.valignment="CENTER" />
            <Label fx:id="testLabel" text="Label" GridPane.rowIndex="5" />
            <Button fx:id="pickBtn" mnemonicParsing="false" onAction="#pickPokemon" prefHeight="34.0" prefWidth="83.0" text="Pick" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="3" GridPane.valignment="TOP">
               <GridPane.margin>
                  <Insets top="10.0" />
               </GridPane.margin>
            </Button>
         </children>
      
      </GridPane>
   </children>
   <stylesheets>
      <URL value="@../../../css/newcss.css" />
   </stylesheets>
</AnchorPane>